<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.course.server.dao.ChapterDao">
  <resultMap id="BaseResultMap" type="com.course.server.entity.Chapter">
    <id column="id" jdbcType="CHAR" property="id" />
    <result column="course_id" jdbcType="CHAR" property="courseId" />
    <result column="name" jdbcType="VARCHAR" property="name" />
  </resultMap>

  <select id="selectChapterAll" resultMap="BaseResultMap">
    SELECT id,course_id,name from chapter
  </select>
</mapper>